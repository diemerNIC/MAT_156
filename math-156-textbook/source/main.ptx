          <example>
            <statement>
              Simplify <m>(p\wedge q)\vee(\sim\!p\,\wedge q)</m>.
            </statement>
            <solution>
              <p>
                <tabular top="major" halign="center" left="medium">
                  <row bottom="medium">
                    <cell right="medium">
                      <m>p</m>
                    </cell>
                    <cell right="medium">
                      <m>q</m>
                    </cell>
                    <cell right="medium">
                      <m>\sim\!p</m>
                    </cell>
                    <cell right="medium">
                      <m>p\wedge q</m>
                    </cell>
                    <cell right="medium">
                      <m>\sim\!p\,\wedge q</m>
                    </cell>
                    <cell right="medium">
                      <m>(p\wedge q)\vee(\sim\!p\,\wedge q)</m>
                    </cell>
                  </row>
                  <row>
                    <cell right="medium">
                      0
                    </cell>
                    <cell right="medium">
                      0
                    </cell>
                    <cell right="medium">
                      1
                    </cell>
                    <cell right="medium">
